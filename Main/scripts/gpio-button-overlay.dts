/dts-v1/;
/plugin/;
/ {
	compatible = "brcm,bcm2835", "brcm,bcm2708", "brcm,bcm2709";

	fragment@0 {
		target-path = "/soc";
		__overlay__ {
			keypad: keypad {
				compatible = "gpio-keys";
				#address-cells = <1>;
				#size-cells = <0>;
				button@13 {
					label = "Front bumper left";
					linux,code = <49>;
					linux,input-type = <1 22>;
					gpios = <&gpio 13 0>;
				};
				button@20 {
					label = "Shutdown button";
					linux,code = <116>;
					linux,input-type = <1 22>;
					gpios = <&gpio 20 0>;
				};
				button@21 {
					label = "Front bumper right";
					linux,code = <49>;
					linux,input-type = <1 22>;
					gpios = <&gpio 21 0>;
				};
			};
		};
	};

	fragment@1 {
		target = <&leds>;
		__overlay__ {
			ledgreen: ledgreen {
				label = "LedGreen";
				gpios = <&gpio 10 1>;
				linux,default-trigger = "mmc0";
			};

			ledblue: ledblue {
				label = "LedBlue";
				gpios = <&gpio 11 1>;
				linux,default-trigger = "none";
			};
		};
	};

	fragment@2 {
		target-path = "/";
		__overlay__ {
			power_ctrl: power_ctrl {
				compatible = "gpio-poweroff";
				gpios = <&gpio 9 1>;
			};
		};
	};
};
